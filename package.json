{
  "name": "redux-analytics-manager",
  "version": "2.1.4",
  "description": "Redux middleware for registering actions to analytics events.",
  "author": "Rob Gonnella",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robgonnella/redux-analytics-manager.git"
  },
  "keywords": [
    "redux",
    "react",
    "react-redux",
    "redux-analytics",
    "google-analytics",
    "analytics",
    "analytics-manager"
  ],
  "main": "dist/index.min.js",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf dist umd index.d.ts",
    "lint": "tslint src/*",
    "prepublishOnly": "npm run test && npm run build",
    "build:declaration": "tsc --declaration true --emitDeclarationOnly --outDir .",
    "minify": "uglifyjs dist/index.js --compress --mangle --output dist/index.min.js",
    "minify:umd": "uglifyjs umd/index.js --compress --mangle --output umd/index.min.js",
    "build": "npm run clean && rollup -c && npm run build:declaration && npm run minify && npm run minify:umd",
    "test": "npm run build && nyc mocha -r ts-node/register test/*.ts",
    "posttest": "npm run lint"
  },
  "files": [
    "dist",
    "umd",
    "index.d.ts"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": "90",
    "include": [
      "src"
    ],
    "extension": [
      ".ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.4",
    "@types/sinon": "^4.3.1",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^14.1.1",
    "redux": "^4.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sinon": "^5.0.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^3.1.1",
    "uglify-js": "^3.3.25"
  }
}
