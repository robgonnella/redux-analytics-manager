{
  "name": "redux-analytics-manager",
  "version": "2.1.2",
  "description": "Redux middleware for registering actions to analytics events.",
  "author": "Rob Gonnella",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robgonnella/redux-analytics-manager.git"
  },
  "keywords": [
    "redux",
    "react",
    "react-redux",
    "redux-analytics",
    "google-analytics",
    "analytics",
    "analytics-manager"
  ],
  "main": "commonjs/index.js",
  "module": "module/index.js",
  "types": "./index.d.ts",
  "npmName": "redux-analytics-manager",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "*.js"
      ]
    }
  ],
  "scripts": {
    "clean": "rimraf dist commonjs module index.d.ts",
    "lint": "tslint src/*",
    "prepublishOnly": "npm run test && npm run build",
    "watch": "tsc --watch",
    "build:module": "tsc --target es5 --module es6 --outDir module && npm run build:declaration",
    "build:declaration": "tsc --declaration true --emitDeclarationOnly --outDir .",
    "build:commonjs": "tsc --target es5 --module commonjs --outDir commonjs",
    "minify-umd": "uglifyjs dist/index.js --compress --mangle --output dist/index.min.js",
    "build": "npm run clean && npm run build:module && npm run build:commonjs && rollup -c && npm run minify-umd",
    "test": "nyc mocha -r ts-node/register test/*.ts",
    "posttest": "npm run lint"
  },
  "files": [
    "dist",
    "module",
    "commonjs",
    "index.d.ts"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": "90",
    "include": [
      "src"
    ],
    "extension": [
      ".ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.4",
    "@types/sinon": "^4.3.1",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^11.7.3",
    "redux": "^4.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "sinon": "^5.0.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^3.1.1",
    "uglify-js": "^3.3.25"
  }
}
